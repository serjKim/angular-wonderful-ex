{
  "name": "angular-wonderful-ex",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "gen": "ng generate",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "eslint": "eslint --max-warnings 0 ./src",
    "eslint:fix": "eslint --fix --max-warnings 0 ./src",
    "prettier:check": "prettier --write --config .prettierrc --check \"src/**/*.{ts,scss,html}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@fontsource/material-icons": "^4.5.0",
    "@fontsource/open-sans": "^4.5.2",
    "rxjs": "^7.5.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^12.20.39",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.4.2",
    "jest": "^27.4.5",
    "jest-preset-angular": "^11.0.1",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "typescript": "~4.5.4",
    "husky": "^7.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,scss,html}": [
      "prettier --write"
    ],
    "src/**/*.{ts,html}": [
      "eslint --fix --max-warnings 0"
    ]
  }
}
